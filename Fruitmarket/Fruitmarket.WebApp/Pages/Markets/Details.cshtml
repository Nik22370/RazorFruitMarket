@page "{guid?}"
@model Fruitmarket.WebApp.Pages.Markets.DetailsModel


// Frage an den Paul wieso Name nicht geht?
<h3>Details Page von @Model.Market</h3>

<p>Anzahl der Produkte: @Model.Products.Count</p>

<table class="table table-sm">
    <thead>
    <tr>

        <th>Name</th>
        <th>Price</th>
      

    </tr>
    </thead>

    <tbody>
    @foreach (var p in Model.Products)
    {
        <tr>

            <td>@p.Name</td>
            <td>@p.Price</td>
            
           
            
        </tr>
    }
    </tbody>
    
</table>


